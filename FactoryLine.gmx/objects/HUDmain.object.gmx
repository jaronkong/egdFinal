if (tSeconds &lt; 10) {toPrint = string_insert("0",toPrint,0);}
draw_set_valign( fa_center );
draw_set_halign( fa_left );
draw_text_transformed( x + 320 + 695, y - 134, ":" + toPrint, 0.35, 0.35, 0 );
    
