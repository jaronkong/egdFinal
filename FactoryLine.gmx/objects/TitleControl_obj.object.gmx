    wallPos += wallSpeed;
    if (wallPos &gt; room_height) {
        wallPos = room_height;
    }
}

if (wallPos &gt; logo.bbox_top) {
    logo.y = wallPos + logo.sprite_height;
}

if (logo.y &gt;= room_height) {
    logo.y = room_height
    
    var scale = (room_height - wallPos) / logo.sprite_height;
    if (scale != 0) {logo.image_yscale = scale;}
    else {
        logo.visible = false;
        transition = true;
    }
}

if (transition) {
    timer --;
}

if (timer &lt;= 0) {
    room_goto_next();
}
