            <kind>1</kind>
            <string>///Drag control

if ( mouse_check_button( mb_left ) ) {
    if ( mouse_x &gt; x - heldItem.sprite_width/2 &amp;&amp; mouse_x &lt; x + heldItem.sprite_width/2
        &amp;&amp; mouse_y &gt; y - heldItem.sprite_height/2 &amp;&amp; mouse_y &lt; y + heldItem.sprite_height/2 ) {
        
        if ( !held ) {
            //Grabbing for first time
            grabOffset = grabOffsetMax;
            audio_play_sound( craneSlam_sd, 1, false );
        }
        held = true;   
    }
}
else {
    if ( held ) {
        //Releasing item
        audio_play_sound( placeItem_sd, 1, false );
        instance_create( targetX, targetY, heldItem ); 
        instance_destroy();
    }
    held = false;
    
}

</string>
