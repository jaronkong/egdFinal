            <string>///Draw quotas

draw_set_color( c_white );
draw_set_alpha( 1 );
draw_set_font( counter_ft );
draw_set_halign( fa_right );
draw_set_valign( fa_middle );

var tIndex = 0;
for ( var i = 0; i &lt; ds_grid_width( quota ); ++i ) {
    var tItem = quota[# i, Quota.itemID];
    if ( tItem &lt; 0 ) continue;
    var tRequired = quota[# i, Quota.required];
    var tProduced = quota[# i, Quota.produced];
    var tString = ( string( tProduced ) + " / " + string( tRequired ) );
    draw_text( room_width - 128, 256 + 100 * tIndex, tString );
    itemDraw( tItem, room_width - 64, 256 + 100 * tIndex, get_timer()/1000000+i, c_white, 2, 2 );
    tIndex += 1;
}

draw_set_color( c_white );
draw_set_alpha( 1 );
draw_set_halign( fa_left );
draw_set_valign( fa_top );
</string>
