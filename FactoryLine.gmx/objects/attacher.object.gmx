            <kind>1</kind>
            <string>///Update plunger

if ( plungerHeight &lt; plungerGoal ) {
    plungerHeight = lerp( plungerHeight, plungerGoal, 0.3 );
    if ( plungerHeight &gt;= plungerGoal ) {
        plungerHeight = plungerGoal;
        plungerGoal = 0;
        //Try a recipe
        var cx = gridFromRoomX( x );
        var cy = gridFromRoomY( y );
        var tInst = gridItemAtPoint(
            gridToRoomX( cx ), gridToRoomY( cy + 1 ) );
        if ( instance_exists( tInst ) || ( itemAdd &gt;= 0 ) ) {
            var tResultY = gridToRoomY( cy + 1 );
            var tItemBase = -1;
            var tColorBase = c_black;
            if ( instance_exists( tInst ) ) {
                tItemBase = tInst.itemID;
                tColorBase = tInst.image_blend;
                tResultY = tInst.y;
                with ( tInst ) instance_destroy( );
            }
            var tResult = max( tItemBase, itemAdd );
            var tResultColor = max( tColorBase, itemAddColor );
            if ( ( tItemBase &gt;= 0 ) &amp;&amp; ( itemAdd &gt;= 0 ) ) {
                tResult = recipeResult(
                    object_get_name( object_index ),
                    tItemBase, itemAdd );
                tResultColor = itemColorMerge( tColorBase, itemAddColor, 0.2 );
            }
            var tNew = itemCreate( gridToRoomX( cx ), tResultY, tResult );
            tNew.image_blend = tResultColor;
        }
