        cellSolid[1] = GridSolid.Open;
    }
} else if ( plungerHeight &gt; plungerGoal ) {
    plungerHeight = lerp( plungerHeight, plungerGoal, 0.2 );
    if ( plungerHeight &lt;= plungerGoal ) {
