          <argument>
            <kind>1</kind>
            <string>///Load attachment recipes

var tFile = ( working_directory + "/recipes/attachments.dat" );

var tBase, tBaseCount = 0;
var tAttach, tAttachCount = 0;
var tBaseAttach = false;

//Load in base parts and attachable parts
ini_open( tFile );
while ( ini_key_exists( "attach", string( tAttachCount ) ) ) {
    tAttach[tAttachCount] = ini_read_string( "attach", string( tAttachCount ), "" );
    ++tAttachCount;
}
while ( ini_key_exists( "base", string( tBaseCount ) ) ) {
    tBase[tBaseCount] = ini_read_string( "base", string( tBaseCount ), "" );
    for ( var i = 0; i &lt; tAttachCount; ++i ) {
        tBaseAttach[tBaseCount, i] = ini_read_real(
            tBase[tBaseCount], tAttach[i]+"-max", 0 );
    }
    ++tBaseCount;
}
ini_close( );

//Load in item data for each
for ( var i = 0; i &lt; tBaseCount; ++i ) {
    tBase[i] = itemProperty( tBase[i], Item.tag );
}
for ( var i = 0; i &lt; tAttachCount; ++i ) {
    tAttach[i] = itemProperty( tAttach[i], Item.tag );
}

//Generate combinations
var tQueue = ds_queue_create( );
for ( var i = 0; i &lt; tBaseCount; ++i ) {
    //Seed the queue
    var tBaseSeed = false;
    var tDataSeed = false;
    for ( var  j = 0; j &lt; tAttachCount; ++j ) {
        tBaseSeed[j] = 0;
        tDataSeed[j] = tBaseAttach[i, j];
    }
    ds_queue_enqueue( tQueue, tBase[i], tBaseSeed, tDataSeed );
    
    //Attempt all combinations
    while ( !ds_queue_empty( tQueue ) ) {
        var tTagOld = ds_queue_dequeue( tQueue );
        var tBaseOld = ds_queue_dequeue( tQueue );
        var tDataOld = ds_queue_dequeue( tQueue );
        
        for ( var j = 0; j &lt; tAttachCount; ++j ) {
            if ( tDataOld[j] &gt; 0 ) {
                //Copy the data to a new array;
                var tBaseNew = false;
                var tDataNew = false;
                for ( var k = 0; k &lt; tAttachCount; ++k ) {
                    tBaseNew[k] = tBaseOld[k];
                    tDataNew[k] = tDataOld[k];
                }
                
                //Transfer one attachment over
                ++tBaseNew[j];
                --tDataNew[j];
                
                //Define the new item
                var tTagNew = tBase[i];
                for ( var k = 0; k &lt; tAttachCount; ++k ) {
                    if ( tBaseNew[k] &gt; 0 ) {
                        tTagNew += "-" + string( tBaseNew[k] ) + tAttach[k];
                    }
                }
                itemDefine( tTagNew, tTagNew, itemProperty( tTagOld, Item.sprite ) );
                
                //Define a recipe
                recipeDefine( tTagNew, "attacher", tTagOld, tAttach[j] );
                
                //Put it back in the queue
                ds_queue_enqueue( tQueue, tTagNew, tBaseNew, tDataNew );
            }
        }
    }
    
    ds_queue_clear( tQueue );
}
ds_queue_destroy( tQueue );
</string>
